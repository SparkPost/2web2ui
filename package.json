{
  "name": "react-webui",
  "version": "0.1.0",
  "private": true,
  "package-lock": true,
  "engines": {
    "node": ">=10.17"
  },
  "engineStrict": true,
  "scripts": {
    "start": "REACT_APP_VERSION=local-$USER-$(git rev-parse HEAD) node scripts/start.js",
    "serve": "npx serve -s build",
    "build": "node scripts/build.js",
    "analyze": "source-map-explorer build/static/js/main.*",
    "test": "node scripts/test.js --env=jsdom --config=./jest.config.js",
    "test-ci": "CI=true npm test -- --colors --maxWorkers=2",
    "test-integration": "TZ=America/New_York cypress open --config-file cypress.dev.json",
    "test-integration-headless": "TZ=America/New_York cypress run --config-file cypress.dev.json",
    "test-integration-ci": "npm run serve & wait-on http://localhost:5000 && TZ=America/New_York cypress run --config-file cypress.ci.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{config,cypress,src,.eslintplugin}/**/*.js": [
      "eslint --fix"
    ],
    "src/**/*.js": [
      "stylelint"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "lodash",
      "recharts",
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "description": "SparkPost web app built in React",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SparkPost/2web2ui.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/SparkPost/2web2ui/issues"
  },
  "jest-junit": {
    "outputDirectory": "test_results/junit",
    "addFileAttribute": "true"
